#include <stdio.h>
#include "holberton.h"

/**
 *print_to_98 - print numbers until 98
 *
 *@n: variable
 *
 *Return: result (success)
 */

void print_to_98(int n)
{
	while (n != 98)
	{
		if (n < 98)
		{
			printf("%d", n);
			_putchar(',');
			_putchar(' ');
			n++;
		}
		else if (n > 98)
		{
			printf("%d", n);
			_putchar(',');
			_putchar(' ');
			n++;
		}


		_putchar('\n');
	}

	_putchar('\n');


}
