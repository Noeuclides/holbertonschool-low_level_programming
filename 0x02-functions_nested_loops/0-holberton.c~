#include "holberton.h"
/**
 *main - entry point
 *
 *_putchar: put char
 *
 *return: 0 if correct
 */

int main(void)
{
	char c;

	c = 'H';
	_putchar(c);

	c = 'o';
	_putchar(c);

	c = 'l';
	_putchar(c);

	c = 'b';
	_putchar(c);

	c = 'e';
	_putchar(c);

	c = 'r';
	_putchar(c);

	c = 't';
	_putchar(c);

	c = 'o';
	_putchar(c);

	c = 'n';
	_putchar(c);

	_putchar('\n');

	return (0);
}
