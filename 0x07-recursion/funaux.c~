#include <unistd.h>

/**
 * funaux - writes the character c to stdout
 * @c: The character to print
 *
 * Return: On success 1.
 * On error, -1 is returned, and errno is set appropriately.
 */
int funaux(int m, int n)
{
	if (m * m == n)
	{
		return (m);
	}
	else if (m * m < n)
	{
		m++;
		funaux(m, n);
		return (m - 1);
	}
	else
		return (1);
}
